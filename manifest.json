{
    "name": "Url Bookmark",
    "description": "A url bookmarker extension",
    "version": "1.1",
    "manifest_version": 3,
    "author": "amanabridget03@gmail.com",
    "action": {
        "default_popup": "./index.html",
        "default_icon": {
            "16": "./img/tabstash_icon_16x16.png",
            "32": "./img/tabstash_icon_32x32.png",
            "48": "./img/tabstash_icon_16x16.png",
            "128": "./img/tabstash_icon_16x16.png"
        }
    },
    "permissions": [
        "tabs"
    ]
}